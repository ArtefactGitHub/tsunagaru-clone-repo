<!DOCTYPE html>
<html>
  <head>
    <title>RPActioncable</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'mypage', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'mypage', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'shared/header' %>

    <div class='main'>
      <%= yield %>
    </div>

    <%= render 'shared/footer' %>
  </body>
</html>
